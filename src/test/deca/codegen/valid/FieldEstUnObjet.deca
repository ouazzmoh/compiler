class A {
	int a = 5;
	void setA(int b){
		this.a = b;
	}
}

class B {
	A a = new A();

	A getA(){
		return this.a;
	}
}
{
	A c;
	B b = new B();
	A a = b.getA();
	println(a.a);
	a.setA(3);
	c = b.getA();
	println(c.a);
}
